    <header class="account">
      <div class="account_box">
        <div class="account_box_left">
          <img src="https://cdna.artstation.com/p/assets/images/images/034/457/364/large/shin-min-jeong-.jpg?1612345089" alt="account_box_upload" class="account_box_img_img" height="200" width="200">
          <p class="account_box_img_para"><button>CHANGE IMAGE</button></p>
        </div>

        <div class="account_box_right">
        <h1>PROFILE SETTINGS</h1>
        <p>
          You can set your prefered settings, display name, create profile
          information and more
        </p>
        </div>
      </div>
    </header>

    <section class="account_info">
        <div class="account_info_box">
            <div class="account_info_left">
                <form action="submit" class="account_info_sLabel">
                    <label for="username">USERNAME</label>
                    <input type="text" id="fname" name="fname" value="Username">
                </form>
                <form action="submit" class="account_info_sLabel">
                    <label for="email">EMAIL</label>
                    <input type="text" id="femail" name="femail" value="EMAIL">
                </form>
                <form action="submit" class="account_info_sLabel">
                    <label for="website">WEBSITE</label>
                    <input type="text" id="fwebsite" name="fwebsite" value="Website">
                </form>
            </div>
            <div class="account_info_box_center">
                <form action="submit" class="account_info_description">
                    <label for="description">DESCRIPTION</label>
                    <textarea name="description" id="description" cols="100" rows="20">Describe Yourself</textarea>
                </form>
            </div>
            <div class="account_info_right">
                <form action="submit" class="account_info_sLabel">
                    <label for="instagram">INSTAGRAM</label>
                    <input type="text" id="socialI" name="socialI" value="Instagram">
                </form>
                <form action="submit" class="account_info_sLabel">
                    <label for="instagram">GITHUB</label>
                    <input type="text" id="socialI" name="socialI" value="GitHub">
                </form>
                <form action="submit" class="account_info_sLabel">
                    <label for="instagram">REDDIT</label>
                    <input type="text" id="socialI" name="socialI" value="Reddit">
                </form>
                <form action="submit" class="account_info_sLabel">
                    <label for="instagram">LINKEDIN</label>
                    <input type="text" id="socialI" name="socialI" value="LinkeDin">
                </form>
            </div>
        </div>
        <div class="account_info_upload">
            <div class="account_info_upload_box">
                <h3>DONE?</h3>
                <button>UPLOAD</button>
            </div>
        </div>
    </section>

