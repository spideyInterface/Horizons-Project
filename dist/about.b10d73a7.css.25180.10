nav {
  color: #d6d6d5;
  z-index: 10;
  background-color: #111;
  justify-content: space-between;
  width: 100%;
  font-family: Expose, sans-serif;
  font-style: normal;
  text-decoration: none;
  display: flex;
  position: fixed;
  top: 0%;
}

nav h3 {
  color: #d6d6d5;
  padding-top: .9rem;
  padding-left: .5rem;
  font-family: Expose, sans-serif;
  font-style: normal;
  text-decoration: none;
}

nav menu {
  list-style: none;
  display: flex;
}

nav menu a {
  color: #d6d6d5;
  font-optical-sizing: auto;
  padding: .938rem;
  font-family: Expose, sans-serif;
  font-size: 1rem;
  font-style: normal;
  text-decoration: none;
  transition: all .3s ease-out;
  display: inline-block;
}

nav menu a:hover {
  cursor: pointer;
  text-decoration: underline .2rem #e7e6e3;
}

nav menu .closeMenu {
  cursor: pointer;
  display: none;
}

nav .openMenu {
  cursor: pointer;
  margin: 1.25rem;
  display: none;
}

footer {
